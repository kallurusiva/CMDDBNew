/* Simple CSS menu for ebook Categories */

.menu_simple {
    vertical-align: top;
}

.menu_simple ul {
    margin: 0; 
    padding: 0;
    width:150px;
    list-style-type: none;
}


.menu_simple ul li {

    border-bottom: 1px dotted #2084F4; 

}
.menu_simple ul li a {
    text-decoration: none;
    color: #2084F4; 
    padding: 5px 5px;
    background-color: #FFFFFF;
    display:block;
    font: 12px  "Open Sans", Arial, Verdana, sans-serif;
    cursor: pointer; 
}
 
/*.menu_simple ul li a:visited {
    color: white;
}*/
 
.menu_simple ul li a:hover {
    color: white;
    background-color: #FB935F;
    cursor: pointer; 

    border-top: 1px solid #687367;
	border-bottom: 1px solid  #687367;
	

}

.menu_simple ul li .current {
    color: white;
    font-weight: bold;
    font-size: 13px; 
    font-variant:small-caps; 
    background-color: #FB935F;
    cursor: pointer; 
    border-top: 1px solid #687367;
	border-bottom: 1px solid  #687367;

}



